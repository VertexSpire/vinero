  "scripts": {
    "start": "node index.js",
    "dev": "tsc-watch --onSuccess \"npm run transpile:dev\"",
    "inspect": "tsc-watch --onSuccess \"npm run transpile:inspect\"",
    "transpile": "tsc && tsc-alias",
    "transpile:clean": "rimraf app/api/v2/dist && npm run transpile",
    "transpile:dev": "tsc-alias && npm run start:dev",
    "transpile:inspect": "tsc-alias && npm run start:inspect",
    "start:dev": "nodemon index.js",
    "start:inspect": "nodemon --inspect index.js",
    "u-coverage": "THM_NODE_PROD_TYPE=tests nyc --reporter=html --reporter=text --exclude 'app/views/' --delay --exclude 'app/database' --exclude 'tests' --exclude 'config.js' mocha tests/unit --recursive --exit --parallel",
    "u-test": "THM_NODE_PROD_TYPE=tests mocha tests/unit --recursive --exit",
    "i-test-routes": "THM_NODE_PROD_TYPE=tests mocha --timeout 60000 tests/integration/routes/ --file tests/integration/routes/pages/db.js --recursive --exit",
    "i-test-db": "THM_NODE_PROD_TYPE=tests mocha --timeout 10000 tests/integration/database/ --file tests/integration/database/db.js --recursive --exit",
    "u-test-watch": "THM_NODE_PROD_TYPE=tests mocha tests/unit --recursive --watch",
    "i-test-routes-watch": "THM_NODE_PROD_TYPE=tests mocha tests/integration/routes/ --file tests/integration/routes/pages/db.js --recursive --watch",
    "i-test-db-watch": "THM_NODE_PROD_TYPE=tests mocha tests/integration/database/ --file tests/integration/database/db.js --recursive --watch",
    "test": "npm run transpile:clean && NODE_ENV=testing jest --forceExit --testTimeout=40000 --maxWorkers=50%",
    "test:ci": "NODE_ENV=testing jest --forceExit --testTimeout=40000 --maxWorkers=50% --coverage --bail",
    "test:verbose": "npm run transpile:clean && NODE_ENV=testing jest --forceExit --verbose --detectOpenHandles --watch --onlyChanged",
    "test:troubleshoot": "npm run transpile:clean && NODE_ENV=testing jest --debug --detectOpenHandles --forceExit",
    "test:coverage": "npm run transpile:clean && NODE_ENV=testing jest --coverage",
    "test:watch": "npm run transpile:clean && NODE_ENV=testing jest --watch --testTimeout=40000 --maxWorkers=50%",
    "prettier:fix": "prettier --write \"./app/api/**/*.{ts,js,json}\"",
    "prettier:check": "prettier --check \"./app/api/**/*.{ts,js,json}\"",
    "lint": "eslint --no-error-on-unmatched-pattern --ext .js,.ts ./app/**/*",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,js,json,css,html}": "prettier --write",
    "**/*.{js}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
